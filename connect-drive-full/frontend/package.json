{
  "name": "connect-drive-frontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:property": "jest --config jest.property.config.js",
    "test:property:watch": "jest --config jest.property.config.js --watch",
    "test:property:coverage": "jest --config jest.property.config.js --coverage",
    "test:all": "npm run test:ci && npm run test:property",
    "validate": "npm run type-check && npm run lint && npm run test:all",
    "validate:env": "node ../scripts/validate-environment.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "axios": "^1.5.0",
    "clsx": "^2.1.1",
    "next": "14.2.33",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "24.10.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axe-core": "^4.7.0",
    "eslint": "^8.45.0",
    "eslint-config-next": "14.2.33",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-check": "^3.12.0",
    "jest": "^29.6.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.6.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "5.9.3"
  }
}
